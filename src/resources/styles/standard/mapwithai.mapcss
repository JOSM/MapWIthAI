/*******************************************
 * Set .mapwithai for all known MapWithAI datasets *
 *******************************************/
way[source=~/^(?i)(microsoft|digitalglobe)$/] {
    set .mapwithai;
}

way.mapwithai {
    color: fuchsia;
}
way.mapwithai:closed2 {
    fill-color: fuchsia;
}
