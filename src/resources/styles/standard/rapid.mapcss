/*******************************************
 * Set .rapid for all known RapiD datasets *
 *******************************************/
way[source=~/^(?i)(microsoft|digitalglobe)$/] {
    set .rapid;
}

way.rapid {
    color: fuchsia;
}
way.rapid:closed2 {
    fill-color: fuchsia;
}
